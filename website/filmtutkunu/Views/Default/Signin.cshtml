
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@200&display=swap" rel="stylesheet">
    <script src="https://kit.fontawesome.com/4114cf82b1.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="~/signin.css">

    <!-- <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
    crossorigin="anonymous"> -->


    <title>Signin</title>
</head>

<body>
    <div class="container">

        <div class="fieldset">
            <form method="post">

                <h2><span class="oto-input"></span> </h2>
                <table>

                    <tr>
                        <td><label for="name">Ad :</label></td>
                        <td><input type="text" name="isim" placeholder="Your Name"></td>
                    </tr>
                    <tr>
                        <td><label for="surname">Soyad :</label></td>
                        <td><input type="text" name="soyisim" placeholder="Your Surname"></td>
                    </tr>
                    <tr>
                        <td><label for="username">Kullanıcı Adı :</label></td>
                        <td><input id="kullaniciAdi" type="text" name="kullaniciadi" placeholder="Your Username"></td>
                    </tr>
                    <tr>
                        <td><label for="eposta">E-Mail :</label></td>
                        <td><input type="email" name="email" placeholder="Your E-mail Address"></td>
                    </tr>
                    <tr>
                        <td><label for="password">Şifre :</label></td>
                        <td><input type="password" name="şifre" placeholder="Password"></td>
                    </tr>
                    <tr>
                        <td><label for="">Şifre :</label></td>
                        <td><input type="password" name="şifre" placeholder="Verify"></td>
                    </tr>

                    <tr>
                        <td><label for="date">Doğum Tarihi :</label></td>
                        <td><input type="date" name="dogumtarihi"></td>
                    </tr>


                </table>





                <button type="submit" class="sub-button" id="but">Kayıt Ol</button>
            </form>
</body>
</html>
